{
  "manifest_version": 2,

  "name": "Omar's a goon",
  "description": "Agam KAKA",
  "version": "1.666",

  "content_scripts": [
    {
      "matches": ["http://*.com/*"],
      "js": ["jquery-2.2.0.min.js", "popup.js", "hound-web-sdk.min.js"]
    }
  ],

  "browser_action": {
   "default_icon": "icon2.png",
   "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab"
   ]
}
